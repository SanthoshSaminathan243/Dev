{
   "agent":{
      "metrics_collection_interval":180,
      "logfile":"/opt/aws/amazon-cloudwatch-agent/logs/amazon-cloudwatch-agent.log"
   },
   "metrics":{
      "metrics_collected":{
         "processes":{
            "measurement":[
               "running",
               "sleeping",
               "dead"
            ]
         },
         "disk":{
            "resources":[
               "/"
            ],
            "measurement":[
               {
                  "name":"free",
                  "rename":"Disk_Free",
                  "unit":"Gigabytes"
               },
               {
                  "name":"total",
                  "rename":"Disk_Total",
                  "unit":"Gigabytes"
               },
               {
                  "name":"used",
                  "rename":"Disk_Used",
                  "unit":"Gigabytes"
               }
            ],
            "ignore_file_system_types":[
               "sysfs",
               "devtmpfs",
               "tmpfs"
            ],
            "metrics_collection_interval":60
         },
         "swap":{
            "measurement":[
               {
                  "name":"swap_used",
                  "rename":"Swap_Used",
                  "unit":"Gigabytes"
               },
               {
                  "name":"swap_free",
                  "rename":"Swap_Cached",
                  "unit":"Gigabytes"
               },
               {
                  "name":"swap_used_percent",
                  "rename":"Swap_Used_Percent",
                  "unit":"Percent"
               }
            ],
            "metrics_collection_interval":60
         },
         "mem":{
            "measurement":[
               {
                  "name":"mem_used",
                  "rename":"Memory_Used",
                  "unit":"Gigabytes"
               },
               {
                  "name":"mem_cached",
                  "rename":"Memory_Cached",
                  "unit":"Gigabytes"
               },
               {
                  "name":"mem_total",
                  "rename":"Memory_Total",
                  "unit":"Gigabytes"
               }
            ],
            "metrics_collection_interval":60
         }
      },
      "append_dimensions":{
         "ImageId":"${aws:ImageId}",
         "InstanceId":"${aws:InstanceId}",
         "InstanceType":"${aws:InstanceType}",
         "AutoScalingGroupName":"${aws:AutoScalingGroupName}"
      },
      "aggregation_dimensions":[
         [
            "InstanceId",
            "InstanceType"
         ],
         [

         ]
      ]
   },
   "logs":{
      "logs_collected":{
         "files":{
            "collect_list":[
               {
                  "file_path":"/var/log/httpd/*access_log*",
                  "log_group_name":"APP-CWAgent-TestLogGroup",
                  "log_stream_name":"APP-APP1-Access-Log-{instance_id}",
                  "timestamp_format":"%Y-%m-%d %H:%M:%S"
               },
               {
                  "file_path":"/var/log/httpd/*error_log*",
                  "log_group_name":"APP-CWAgent-TestLogGroup",
                  "log_stream_name":"APP-APP1-Error-Log-{instance_id}",
                  "timestamp_format":"%Y-%m-%d %H:%M:%S"
               },
               {
                  "file_path":"/var/www/storage/logs/*laravel*.log",
                  "log_group_name":"APP-CWAgent-TestLogGroup",
                  "log_stream_name":"APP-APP2-Laravel-Log-{instance_id}",
                  "timestamp_format":"%Y-%m-%d %H:%M:%S"
               },
               {
                  "file_path":"/var/www/storage/logs/*event*.log",
                  "log_group_name":"APP-CWAgent-TestLogGroup",
                  "log_stream_name":"APP-APP2-Event-Log-{instance_id}",
                  "timestamp_format":"%Y-%m-%d %H:%M:%S"
               },
               {
                  "file_path":"/var/www/storage/logs/*worker*.log",
                  "log_group_name":"APP-CWAgent-TestLogGroup",
                  "log_stream_name":"APP-APP2-Worker-Log-{instance_id}",
                  "timestamp_format":"%Y-%m-%d %H:%M:%S"
               },
               {
                  "file_path":"/var/www/storage/logs/*horizon*.log",
                  "log_group_name":"APP-CWAgent-TestLogGroup",
                  "log_stream_name":"APP-APP2-Horizon-Log-{instance_id}",
                  "timestamp_format":"%Y-%m-%d %H:%M:%S"
               }
            ]
         }
      }
   }
}